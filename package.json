{
    "version": "0.1.5",
    "license": "MIT",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "files": [
        "dist",
        "src"
    ],
    "engines": {
        "node": ">=10"
    },
    "scripts": {
        "start": "tsdx watch",
        "build": "tsdx build",
        "test": "tsdx test --passWithNoTests",
        "lint": "tsdx lint",
        "prepare": "tsdx build",
        "size": "size-limit",
        "analyze": "size-limit --why",
        "format": "prettier --write \"./**/*.{ts,tsx}\""
    },
    "peerDependencies": {
        "react": ">=16"
    },
    "husky": {
        "hooks": {
            "pre-commit": "tsdx lint"
        }
    },
    "prettier": {
        "printWidth": 200,
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "trailingComma": "es5"
    },
    "name": "scrape-recipe-schema",
    "author": "arcetros",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/arcetros/scrape-recipe-schema.git"
    },
    "bugs": {
        "url": "https://github.com/arcetros/scrape-recipe-schema"
    },
    "homepage": {
        "url": "https://github.com/arcetros/scrape-recipe-schema"
    },
    "keywords": [
        "recipe",
        "food-recipe",
        "scraper",
        "json-ld",
        "jsonld",
        "micro-data",
        "microdata"
    ],
    "module": "dist/scrape-recipe-schema.esm.js",
    "size-limit": [
        {
            "path": "dist/scrape-recipe-schema.cjs.production.min.js",
            "limit": "10 KB"
        },
        {
            "path": "dist/scrape-recipe-schema.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@size-limit/preset-small-lib": "^8.1.0",
        "@types/cheerio": "^0.22.31",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.11",
        "husky": "^8.0.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "size-limit": "^8.1.0",
        "tsdx": "^0.14.1",
        "tslib": "^2.4.1",
        "typescript": "^4.9.3"
    },
    "dependencies": {
        "axios": "^1.2.0",
        "domino": "^2.1.6",
        "jsonschema": "^1.4.1",
        "microdata-node": "^2.0.0"
    }
}
