{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "A recipe scraped from the web",
    "type": "object",
    "required": ["name", "recipeIngredients"],
    "properties": {
        "name": {
            "type": "string",
            "minLength": 1
        },
        "recipeIngredients": {
            "type": "array",
            "minItems": 1,
            "items": { "type": "string" }
        }
    }
}
